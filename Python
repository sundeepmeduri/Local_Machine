---------------------------------------------------
list:

list1 = ['1','2','3']
print(list1)
print(list1[1])
list1.append('4')
print(list1)
print(list1[1])
del list1[1]
print(list1)
print(list1[1])
print(list1[1:2])
print(list1[2:2])
print(list1[0:1])
